body{
  background: #F0F0F0 url(<%= asset_path 'bg.png' %>) repeat-x 0 0;
}

.prev{
	background: url(<%= asset_path 'circles.png' %>) left top no-repeat;
}

.next {
	background: url(<%= asset_path 'circles.png' %>) center top no-repeat;
}

#close{
	background: url(<%= asset_path 'circles.png' %>) right top no-repeat;
}